{
  "name": "pulse",
  "description": "",
  "license": "Envato REGULAR LICENSE",
  "homepage": "",
  "bower_src": "../bower_components",
  "ignore": [
    "**/.*",
    "assets",
    "node_modules",
    "test",
    "tests"
  ],
  "copy": [
    {
      "src": [
        "jquery/dist/jquery.js",
        "bootstrap/dist/js/bootstrap.js",
        "tether/dist/js/tether.min.js",
        "sticky-kit/jquery.sticky-kit.min.js",
        "PACE/pace.min.js",
        "jQuery-Storage-API/jquery.storageapi.min.js",
        "jquery-pjax/jquery.pjax.js",
        "screenfull/dist/screenfull.min.js",
        "jquery.stellar/jquery.stellar.min.js",
        "owl.carousel/dist/**",
        "jscroll/jquery.jscroll.min.js",
        "mediaelement/build/mediaelement-and-player.min.js",
        "mediaelement/build/mediaelementplayer.min.css",
        "mediaelement/build/flashmediaelement.swf",
        "mediaelement/build/silverlightmediaelement.xap"
      ],
      "dest": "html/libs",
      "cwd": "../bower_components",
      "expand": true
    },
    {
      "src": [
        "animate.css/animate.min.css",
        "font-awesome/css/**",
        "font-awesome/fonts/**",
        "material-design-icons/iconfont/**",
        "bootstrap/dist/css/**",
        "bootstrap/scss/**"
      ],
      "dest": "html/css",
      "cwd": "../bower_components",
      "expand": true
    }
  ],
  "dependencies": {
    "PACE": "pace#~1.0.2",
    "animate.css": "~3.2.0",
    "bootstrap": "4.0.0-alpha.2",
    "font-awesome": "4.3.0",
    "jQuery-Storage-API": "~1.7.4",
    "jquery": "2.1.3",
    "jquery-pjax": "~1.9.6",
    "jquery.stellar": "~0.6.2",
    "jscroll": "~2.3.5",
    "material-design-icons": "~2.0.0",
    "mediaelement": "~2.20.1",
    "owl.carousel": "~2.1.5",
    "pjax": "~0.2.3",
    "screenfull": "~1.2.1",
    "sticky-kit": "~1.1.2",
    "tether": "~1.1.0"
  }
}
